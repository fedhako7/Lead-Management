{
  "name": "lead-management-backend",
  "version": "1.0.0",
  "description": "Lead Management System Backend",
  "main": "dist/start.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only start.ts",
    "build": "tsc",
    "build:clean": "rimraf dist && tsc",
    "start": "node dist/start.js",
    "start:dev": "ts-node start.ts",
    "lint": "eslint **/*.ts",
    "check": "tsc --noEmit"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "mongoose": "^8.0.3"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.5",
    "rimraf": "^5.0.10",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  }
}
